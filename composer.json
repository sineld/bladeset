{
    "name": "sineld/bladeset",
    "description": "A very simple blade extension which allows variables to be set within blade templates.",
    "keywords": ["laravel", "blade", "bladeset", "template"],
    "homepage": "https://github.com/sineld/bladeset",
    "license": "MIT",
    "authors": [{
        "name": "Sinan Eldem",
        "email": "sineld@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.*|6.*|7.*|8.*"
    },
    "support": {
        "issues": "https://github.com/sineld/bladeset/issues"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sineld\\BladeSet\\BladeSetServiceProvider"
            ]
        }
    }
}
